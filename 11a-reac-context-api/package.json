{
  "name": "11a-reac-context-api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint ./src/**/*.{js,jsx} --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --coverage --watchAll"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.395.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.17.5",
    "@babel/preset-react": "^7.17.4",
    "@babel/preset-typescript": "^7.17.4",
    "@eslint/eslint-plugin": "^0.14.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.2.2",
    "@types/react": "^18.0.2",
    "@types/react-dom": "^18.0.1",
    "@vitejs/plugin-react": "^1.2.5",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.7",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^15.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-environment-jsdom": "^27.4.6",
    "postcss": "^8.4.5",
    "prettier": "^2.5.0",
    "tailwindcss": "^3.0.20",
    "vite": "^2.7.14"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "^@/components/ui/card$": "<rootDir>/src/components/ui/card",
      "^@/lib/(.*)$": "<rootDir>/src/lib/$1"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/",
      "\\.css$"
    ]
  }
}
